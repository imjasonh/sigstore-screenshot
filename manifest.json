{
    "manifest_version": 3,
    "name": "Verifiable screenshots",
    "key": "",
    "description": "Take screenshots that can be cryptographically verified",
    "version": "0.0.1",
    "icons": {},
    "background": {
        "service_worker": "service-worker.js",
        "type": "module"
    },
    "oauth2": {
        "client_id": "344413968647-31l50a3vmasmh1p59rps3fjv2o3ncrae.apps.googleusercontent.com",
        "scopes": [
            "https://www.googleapis.com/auth/userinfo.email"
        ]
    },
    "host_permissions": [
        "https://fulcio.sigstore.dev/api/v2/signingCert"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "commands": {
        "screenshot": {
            "suggested_key": {
                "default": "Ctrl+Shift+6",
                "mac": "Command+Shift+6"
            },
            "description": "Take verifiable screenshot"
        }
    },
    "permissions": [
        "activeTab",
        "identity"
    ]
}
